@model List<Product>

<h1>Product List</h1>

<table class="table table-bordered table-striped">
	<thead>
		<tr>
			<th>Name</th>
			<th>Description</th>
			<th>Price</th>
			<th>Quantity</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var product in Model)    
		{
			<tr>
				<td>@product.Name</td>
				<td>@product.Description</td>
				<td>@product.Price</td>
				<td>@product.Quantity</td>
			</tr>
		}
	</tbody>
</table>